{
  "singleQuote": false,
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrderParserPlugins": [
    "typescript",
    "classProperties",
    "decorators-legacy"
  ],
  "importOrder": ["^@*$", "", "^~", "", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
